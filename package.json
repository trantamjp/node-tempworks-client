{
  "name": "tempworks-client",
  "version": "0.0.1",
  "description": "TempWorks client.",
  "main": "lib/src/main.js",
  "scripts": {
    "precompile-ts": "rm -rf lib/",
    "compile-ts": "tsc --build ./tsconfig.build.json",
    "clean-ts": "tsc --build ./tsconfig.build.json --clean",
    "build-library": "npm run compile-ts && npm run prettier",
    "test": "eslint .; [ $? = 0 ] && jest --clearCache && NODE_ENV=test jest --detectOpenHandles --silent --forceExit",
    "prettier": "prettier --write .",
    "lint:check": "eslint \"src/**/*.{json,js,ts,tsx}\"",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "repository": "git@github.com:trantamjp/node-tempworks-client",
    "type": "git",
    "url": "git+ssh://github.com/trantamjp/node-tempworks-client.git"
  },
  "author": "Tam Tran",
  "license": "ISC",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-plugin": "^6.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nock": "^14.0.0-beta.6",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "openapi-fetch": "^0.9.4",
    "pino": "^8.20.0"
  }
}
